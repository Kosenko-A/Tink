num = int(input())
list_in = list(map(int, input().split()))

uniqie_nums = set(list_in)

list_counts = []

for i in uniqie_nums:
    list_counts.append(list_in.count(i))

good_counts = []
good_counts_plus = []

good_counts.append(list_counts[0])
for j in range(1, len(list_counts)):
    if list_counts[j] == list_counts[0]:
        good_counts.append(list_counts[j])
    elif not good_counts_plus and list_counts[j] - list_counts[0] == 1:
        good_counts_plus.append(list_counts[j])

res_list = []
res_list_plus = []
d_nums_cunt = dict(zip(uniqie_nums, list_counts))

for t in d_nums_cunt.items():
    if t[1] == good_counts[0]:
        res_list.append(t[0])
    elif not res_list_plus and t[1] == good_counts_plus[0]:
        res_list_plus.append(t[0])

fin_keys = res_list + res_list_plus
for_res_length = []

for y in list_in:
    if y in fin_keys:
        for_res_length.append(y)

print(for_res_length)
print(len(for_res_length))
